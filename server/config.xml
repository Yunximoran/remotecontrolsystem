<xml version="1.0">
    <base ip="192.168.31.176" mac="20-1E-88-F5-05-B9">
        <connect>
            <udp>
                <client>8084</client>
                <server>8081</server>
                <broad>8082</broad>
                <multi>8083</multi>       
            </udp>
    
            <tcp>
                <server>9095</server>
                <client>8085</client>
                <client_file>8086</client_file>
            </tcp>
        </connect>

        <performance>
        
            <min_process>5</min_process>
            <max_process>10</max_process>
        </performance>

        <redis_config>
            <host>localhost</host>
            <port>6379</port>
            <datas>
                <item>client_status</item>
                <item>client_message</item>
                <item>heart_packages</item>
                <item>softwarelist</item>
                <item>accounts</item>
                <item>reports</item>
                <item>logs</item>
                <item>waitdones</item>
            </datas>
        </redis_config>

        <dependencys>
            <redis>redis-server ./databasetool/conf/redis-service.conf</redis>
            <view>npm --prefix view run serve</view>
        </dependencys>

        <fastapi>
            <cors>
                https://localhost:8080
                http://localhost:8080
            </cors>
        </fastapi>
    </base>
</xml>